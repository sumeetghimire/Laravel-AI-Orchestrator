{
    "name": "sumeetghimire/laravel-ai-orchestrator",
    "description": "A unified, driver-based AI orchestration layer for Laravel",
    "type": "library",
    "license": "MIT",
    "version": "1.0.3",
    "keywords": ["laravel", "ai", "openai", "anthropic", "gemini", "ollama", "orchestrator"],
    "authors": [
        {
            "name": "Sumeet Ghimire"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0",
        "nesbot/carbon": "^2.0|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Sumeetghimire\\AiOrchestrator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sumeetghimire\\AiOrchestrator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sumeetghimire\\AiOrchestrator\\AiOrchestratorServiceProvider"
            ],
            "aliases": {
                "Ai": "Sumeetghimire\\AiOrchestrator\\Facades\\Ai"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

